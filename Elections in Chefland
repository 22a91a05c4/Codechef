#include <iostream>
using namespace std;

int main() {
	int a,i;
	cin>>a;
	for(i=0;i<a;i++)
	{
	    int x,y,z;
	    cin>>x>>y>>z;
	    if(x>50 && y<=50 && z<=50)
	    {
	        cout<<"A"<<endl;
	    }
	    else if(x<=50 && y>50 && z<=50)
	    {
	        cout<<"B"<<endl;
	    }
	    else if(x<=50 && y<=50 && z>50)
	    {
	        cout<<"C"<<endl;
	    }
	    else
	    {
	        cout<<"NOTA"<<endl;
	    }
	}
	return 0;
}
