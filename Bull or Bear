#include <iostream>
using namespace std;

int main() {
    int a,i;
    cin>>a;
    for(i=0;i<a;i++)
    {
        int p,q;
        cin>>p>>q;
        if(p>q)
        {
            cout<<"LOSS"<<endl;
        }
        else if(p<q)
        {
            cout<<"PROFIT"<<endl;
        }
        else
        {
            cout<<"NEUTRAL"<<endl;
        }
    }
	return 0;
}
